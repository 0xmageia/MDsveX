    "add": "^2.0.6",
    "babel-jest": "^24.9.0",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "highlight.js": "^9.15.10",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
