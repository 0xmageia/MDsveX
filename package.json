{
	"name": "mdsvex",
	"version": "0.4.0",
	"description": "Markdown preprocessor for Svelte",
	"main": "dist/main.cjs.js",
	"module": "dist/main.es.js",
	"repository": "https://github.com/pngwn/MDsveX",
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint 'src/' 'test/' --ext .js,.ts",
		"test": "node -r esm ./test/scripts/test_run.js | tap-min",
		"test:watch": "node -r esm ./test/scripts/test_watch.js --verbose",
		"test:cov": "yarn lint && jest --verbose --coverage",
		"postinstall": "patch-package"
	},
	"keywords": [
		"test",
		"preprocessor",
		"mdx",
		"markdown",
		"svelte"
	],
	"author": "pngwn <hello@pngwn.io>",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^12.7.10",
		"cheap-watch": "^1.0.2",
		"eslint": "^6.3.0",
		"eslint-plugin-import": "^2.18.2",
		"esm": "^3.2.25",
		"highlight.js": "^9.15.10",
		"jest-diff": "^24.9.0",
		"kleur": "^3.0.3",
		"patch-package": "^6.2.0",
		"postinstall-postinstall": "^2.0.0",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0",
		"rehype-raw": "^4.0.1",
		"remark-parse": "^7.0.1",
		"remark-rehype": "^5.0.0",
		"rollup": "^1.20.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-svelte": "^5.1.0",
		"svelte": "^3.9.1",
		"unified": "^8.4.1",
		"yarn": "^1.19.0",
		"zora": "^3.0.3"
	},
	"peerDependencies": {
		"svelte": "3.x"
	},
	"dependencies": {
		"front-matter": "^3.0.1",
		"path-browserify": "^1.0.0"
	}
}
